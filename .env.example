# Twitter API Authentication (will be added after grabbing headers)
X_AUTH_TOKEN=your_auth_token
X_COOKIE_STRING=your_cookie_string
X_CSRF_TOKEN=your_csrf_token
BOOKMARKS_API_ID=your_api_id
USER_AGENT=Mozilla/5.0...

# Database Configuration
# Cloud-first option (preferred): use a managed PostgreSQL URL with pgvector enabled.
DATABASE_URL=postgresql://user:password@host:5432/db_name?sslmode=require
DB_CONNECT_TIMEOUT=10
DB_CONNECT_RETRIES=5
DB_CONNECT_RETRY_DELAY=1.0
EMBEDDING_DIM=1536
PGVECTOR_REQUIRED=true
ENABLE_VECTOR_INDEXES=true

# Fallback discrete vars (used only when DATABASE_URL is not set)
POSTGRES_DB=db_name
POSTGRES_USER=your_user
POSTGRES_PASSWORD=your_password
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
